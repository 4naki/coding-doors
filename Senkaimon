@name Senkaimon
#@inputs Open
#@inputs Base:entity
@persist Val1 Val2 Val3 Val4 LoadInt Ready Open Base:entity
@persist [DarkBlue Blue White Black BG]:vector
@model models/sprops/rectangles/size_84/rect_84x84x3.mdl
runOnUse(1)
interval(50)

if(first()){
    Base=entity()
    Base:setAlpha(0)
    
    LoadInt=2000
    
    DarkBlue=vec(35,35,40)*1.5
    Blue=vec(70,70,90)
    White=vec(200)
    Black=vec(0,0,0)
    BG=Black
    
    holoCreate(1)
    holoPos(1,Base:toWorld(vec(0,21,-3)))
    holoAlpha(1,255)
    
    holoCreate(2)
    holoPos(2,Base:toWorld(vec(0,-21,-3)))
    holoAlpha(2,255)
    
    holoCreate(3)
    holoPos(3,Base:toWorld(vec(0,21,-7)))
    holoAlpha(3,255)
    
    holoCreate(4)
    holoPos(4,Base:toWorld(vec(0,-21,-7)))
    holoAlpha(4,255)
    
    for(I=1,4){
        holoModel(I,"models/sprops/rectangles/size_4_5/rect_42x84x3.mdl")
        holoParent(I,Base)
        holoAng(I,Base:toWorld(ang(0,0,0)))
        holoScale(I,vec(0.99,0.98,0.25))
        holoMaterial(I,"models/debug/debugwhite")
        holoColor(I,White)
        holoDisableShading(I,1)
    }

    holoCreate(5)
    holoPos(5,Base:toWorld(vec(0,0,-7)))
    holoParent(5,Base)
    holoAng(5,Base:toWorld(ang(0,0,90)))
    holoModel(5,"models/sprops/misc/tubes_superthin/size_3/tube_6.mdl")
    holoScale(5,vec(2.33,0.8,2.33))
    holoMaterial(5,"models/debug/debugwhite")
    holoColor(5,DarkBlue)
    
    holoCreate(6)
    holoPos(6,Base:toWorld(vec(0,0,-43)))
    holoParent(6,Base)
    holoAng(6,Base:toWorld(ang(0,0,90)))
    holoModel(6,"models/sprops/cuboids/height24/size_1/cube_24x24x24.mdl")
    holoScale(6,vec(3.51,3.51,3.51))
    holoDisableShading(6,1)
    holoMaterial(6,"models/effects/muzzleflash/blurmuzzle")
    holoClipEnabled(6,1,1)
    holoClip(6,1,vec(0,40,0),vec(0,-1,0),0)
    
    holoCreate(7)
    holoPos(7,Base:toWorld(vec(0,0,-7)))
    holoParent(7,Base)
    holoAng(7,Base:toWorld(ang(0,0,90)))
    holoModel(7,"models/sprops/misc/sq_holes/sqhole_d0_24.mdl")
    holoScale(7,vec(3.5,1.1,3.5))
    holoDisableShading(7,1)
    #holoMaterial(7,"models/effects/muzzleflash/blurmuzzle")
    holoColor(7,BG)
    
    holoCreate(105)
    holoPos(105,Base:toWorld(vec(0,0,-7.2)))
    holoParent(105,Base)
    holoAng(105,Base:toWorld(ang(0,0,90)))
    holoModel(105,"models/sprops/misc/sq_holes/sqhole_d0_24.mdl")
    holoMaterial(105,"models/debug/debugwhite")
    holoScale(105,vec(3.5,1.1,3.5))
    holoDisableShading(105,1)
    holoColor(105,vec(255,255,255))
    
    holoCreate(8)
    holoPos(8,Base:toWorld(vec(0,0,-45.5)))
    holoParent(8,Base)
    holoAng(8,Base:toWorld(ang(0,0,90)))
    holoModel(8,"models/sprops/cuboids/height24/size_1/cube_24x24x24.mdl")
    holoScale(8,-vec(3.48,3.2,3.48))
    holoDisableShading(8,1)
    holoMaterial(8,"models/debug/debugwhite")
    holoClipEnabled(8,2,1)
    holoClip(8,2,vec(0,38,0),vec(0,-1,0),0)
    
    holoCreate(9)
    holoPos(9,holoEntity(1):toWorld(vec(0,2.25,-4)))
    holoParent(9,1)
    holoAng(9,Base:toWorld(ang(0,0,0)))
    holoModel(9,"models/sprops/rectangles/size_4_5/rect_42x84x3.mdl")
    holoScale(9,vec(1.01,0.75,1.1))
    holoMaterial(9,"models/effects/muzzleflash/blurmuzzle")
    holoDisableShading(9,1)
    
    holoCreate(10)
    holoPos(10,holoEntity(2):toWorld(vec(0,-2.25,-4)))
    holoParent(10,2)
    holoAng(9,Base:toWorld(ang(0,0,0)))
    holoModel(10,"models/sprops/rectangles/size_4_5/rect_42x84x3.mdl")
    holoScale(10,vec(1.01,0.75,1.1))
    holoMaterial(10,"models/effects/muzzleflash/blurmuzzle")
    holoDisableShading(10,1)

    timer("model",LoadInt)
}
