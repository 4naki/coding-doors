}
if(Ready){
if(Open){
    if(Val3<255){
        Val3+=35
    }
    if(Val3>255){
        Val3=255
    }
    if(Val3==255){
        if(Val4<100){
            Val4+=10
        }
    }
    if(Val4==100){
        for(I=1,6){
            holoAlpha(I+200,100)
        }
    if(Val1<100){
        Val1+=3.5
    }
    if(Val1>100){
        Val1=100
    }
    if(Val1>99){
        if(Val2<90){
            Val2+=3.5
        }
        if(Val2>90){
            Val2=90
        }
    }
    }
}else{
    for(I=1,6){
        holoAlpha(I+200,0)
    }
    if(Val2==0){
        if(Val1>0){
            Val1+=-3.5
        }
        if(Val1<0){
            Val1=0
        }
    }
    if(Val2>0){
        Val2+=-3.5
    }
    if(Val2<0){
        Val2=0
    }
    if(Val1==0){
        if(Val4>0){
            Val4-=10
        }
    }
    if(Val4==0){
        if(Val3>0){
            Val3-=25
        }
        if(Val3<0){
            Val3=0
        }
    }
}

holoPos(1,Base:toWorld(vec(0,21+(Val1/3),-3)))
holoPos(2,Base:toWorld(vec(0,-21-(Val1/3),-3)))
holoPos(3,Base:toWorld(vec(0,21+(Val2/3),-7)))
holoPos(4,Base:toWorld(vec(0,-21-(Val2/3),-7)))

for(I=1,105){
    holoAlpha(I,Val3)
}
}

if(dupefinished()){
    reset()
}
