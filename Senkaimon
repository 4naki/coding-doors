    holoAng(1+200,Base:toWorld(ang(0,0,0)))
    holoParent(1+200,Base)
    holoScale(1+200,vec(6.9,6.9,5))
    holoAlpha(1+200,100)
    holoDisableShading(1+200,1)
    
